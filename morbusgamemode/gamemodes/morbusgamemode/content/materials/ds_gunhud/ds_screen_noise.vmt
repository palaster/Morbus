"UnlitGeneric"
{
	"$basetexture" "ds_gunhud/ds_screen_noise"
	"$vertexalpha" 1
	"$vertexcolor" 1
	"$ignorez" 1
	"$translucent" 1
"Proxies"
  	 {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" 30
      }
   }
}
